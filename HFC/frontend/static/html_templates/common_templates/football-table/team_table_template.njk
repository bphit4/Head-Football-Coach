    {% if pagination.max_pages > 1%}
      <div class="football-table-pagination" style="float: right;">
        {% if pagination.current_page > 1%}
          <button class='btn football-table-pagination-button font12' pagination-action="1">
            First
          </button>
          <button class='btn football-table-pagination-button font12 margin-right-12' pagination-action="{{pagination.current_page - 1}}">
            Prev
          </button>
        {% endif %}
        {% for page_number in pagination.available_page_navigation%}
          <button class='btn football-table-pagination-button font12 
                        {% if page_number == pagination.current_page %} selected {% endif %}' 
                  pagination-action="{{page_number}}">
            {{page_number}}
          </button>
        {% endfor %}
        {% if pagination.current_page < pagination.max_pages%}
          <button class='btn football-table-pagination-button font12 margin-left-12' pagination-action="{{pagination.current_page + 1}}">
            Next
          </button>
          <button class='btn football-table-pagination-button font12' pagination-action="{{pagination.max_pages}}">
            Last 
            <span class="font10">({{pagination.max_pages}})</span>
          </button>
        {% endif %}

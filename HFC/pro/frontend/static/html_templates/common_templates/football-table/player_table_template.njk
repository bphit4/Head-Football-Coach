            <td style='width: 3.125rem; left: 0rem; background-color: #{{player.player_team_season.team_season.team.team_color_primary_hex}};' class='sticky-column'>
              <a href='{{player.player_team_season.team_season.team.team_href}}'>
                <img class='logo logo-30' src='{{player.player_team_season.team_season.team.team_logo}}' />
              </a>
            </td>
            <td style='width: 5.625rem; left: 1.875rem; color:white; background-color: #{{player.player_team_season.team_season.team.team_color_primary_hex}};' class='col-group left-text sticky-column'>
              <a href='{{player.player_team_season.team_season.team.team_href}}'>
                {{player.player_team_season.team_season.team.team_location_name}}
              </a>
            </td>
            {% endif %}
            <!-- BASELINE -->
            <td class='sticky-column' style='width:  1.875rem; left: 0rem; background-color: inherit;' >
              <i class="fa fa-address-card player-profile-popup-icon"></i>
            </td>
            <td style='width: 9.375rem; left: 1.875rem;'class='left-text sticky-column'>
              <a href="{{player.player_href}}">{{player.full_name}}</a>
              {% if player.jersey_number%}
              <span class='font10'>#{{player.jersey_number}}</span>
              {% endif %}
            </td>
            <td>
              <div class='position-badge position-badge-{{player.position}} inline-block'>
                {{player.position}}
              </div>
            </td>
            <td>{{player.player_team_season.ratings.overall.overall|NumberToGradeBadge(100)|safe}}</td>
            <td class="col-group">{{player.player_team_season.age}}</td>
            

            {% if column_controls.Personal.personal.shown %}
              <!-- PERSONAL -->
              <td>{{player.body.height}}</td>
              <td class='col-group'>{{player.body.weight}}</td>
              <td style='width:  7.5rem;' class="col-group left-text">
                <i class="fa fa-map-marker-alt geo-marker" style='padding-right: 0.5rem;' city='{{player.college}}' state='{{player.college}}'></i>
                <span>{{player.college}}</span>
              </td>
            {% endif %}

            {% if column_controls.Stats.games.shown %}
              <!-- GAMES STATS -->
              <td>{{player.player_team_season.season_stats.games.games_played|toLocaleString|OrZero}}</td>
              <td>{{player.player_team_season.season_stats.games.games_started|OrZero}}</td>
              <td>{{player.player_team_season.season_stats.games.plays_on_field|OrZero}}</td>
              <td>{{player.player_team_season.season_stats.games.points|OrZero}}</td>

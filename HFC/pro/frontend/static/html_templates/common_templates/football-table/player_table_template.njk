          <button class='btn team-color-button btn-light football-table-pagination-button font12 margin-right-12' pagination-action="{{pagination.current_page - 1}}">
            Prev
          </button>
        {% endif %}
        {% for page_number in pagination.available_page_navigation%}
          <button class='btn team-color-button btn-light football-table-pagination-button font12 
                        {% if page_number == pagination.current_page %} selected {% endif %}' 
                  pagination-action="{{page_number}}">
